{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "port":9222,
            "name": "Start Chrome",
            "webRoot": "${workspaceFolder}",
            "sourceMaps" : true,
            "trace":true,
            "runtimeExecutable" : "C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe",
            "runtimeArgs":["--remote-debugging-port=9222","--userDataDir=${workspaceFolder}\\.vscode\\chrome","--load-extension=${workspaceFolder}"]
        }
    ]
} 